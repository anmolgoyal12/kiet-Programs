#include<stdio.h>
int add(int,int);
int main()
{
    int n;int i, a,b,c;
    printf("Enter times:\n");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        printf("\nEnter a numbers:\n");
        scanf("%d %d",&a,&b);
        // int c=add(a,b);
    
        printf("sum is : %d\n",add(a,b));
    }    return 0;
        
    
    
}
int add(int a1,int b1)
{
    return (a1+b1);
}
